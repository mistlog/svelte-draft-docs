export default function _() {
    const style = `
        width: 300px;
        border: 1px solid #e6e6e6;
        border-radius: 2px;
        box-shadow: 2px 2px 8px rgba(0, 0, 0, 0.1);
        padding: 1em;
        margin: 0 0 1em 0;
    `;

    <div style={style}>
        <slot></slot>
    </div>
}